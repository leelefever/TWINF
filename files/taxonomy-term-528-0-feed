vieng&quot;&gt;Vang Vieng&lt;/a&gt;, &lt;a href=&quot;/tags/laos&quot;&gt;Laos&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;July 3rd:&lt;/b&gt; &lt;a href=&quot;/tags/phnompenh&quot;&gt;Phnom Penh&lt;/a&gt;, &lt;a href=&quot;/tags/cambodia&quot;&gt;Cambodia&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;July 13th:&lt;/b&gt;&amp;nbsp; &lt;a href=&quot;/tags/siemreap&quot;&gt;Siem Reap&lt;/a&gt;, Cambodia to &lt;a href=&quot;/tags/saigon&quot;&gt;Ho Chi Minh City&lt;/a&gt;, &lt;a href=&quot;/tags/vietnam&quot;&gt;Vietnam&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;July 31st:&lt;/b&gt; &lt;a href=&quot;/tags/hanoi&quot;&gt;Hanoi&lt;/a&gt;, Vietnam to &lt;a href=&quot;/tags/macau&quot;&gt;Macau&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;August 3rd:&lt;/b&gt; Macau to &lt;a href=&quot;/tags/hongkong&quot;&gt;Hong Kong&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;August 8th:&lt;/b&gt; &lt;a href=&quot;/tags/guangzhou&quot;&gt;Guang Zhou&lt;/a&gt;, &lt;a href=&quot;/tags/china&quot;&gt;China&lt;/a&gt;  &lt;/p&gt;&lt;p&gt;&lt;b&gt;August 15th: &lt;/b&gt;&lt;a href=&quot;/tags/Chongqing&quot;&gt;Chongqing&lt;/a&gt;, &lt;a href=&quot;/tags/Yangtze&quot;&gt;Yangtze River&lt;/a&gt;, &lt;a href=&quot;/tags/shanghai&quot;&gt;Shanghai&lt;/a&gt;  &lt;/p&gt;&lt;p&gt;&lt;b&gt;September 9&lt;sup&gt;th&lt;/sup&gt;:&lt;/b&gt;&amp;nbsp; &lt;st1:place&gt;&lt;a href=&quot;/tags/beijing&quot;&gt;&lt;st1:city&gt;Beijing&lt;/st1:city&gt;&lt;/a&gt;,  &lt;st1:country-region&gt;China on &lt;a href=&quot;/tags/trans-siberian&quot;&gt;Trans-Siberian Railway&lt;/a&gt;&lt;br /&gt;&lt;/st1:country-region&gt;&lt;/st1:place&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;September 10&lt;sup&gt;th&lt;/sup&gt;:&lt;/b&gt;&lt;span&gt;&amp;nbsp; &lt;/span&gt;&lt;st1:place&gt;&lt;st1:city&gt;Ulanbaatar&lt;/st1:city&gt;, &lt;a href=&quot;/tags/mongolia&quot;&gt;&lt;st1:country-region&gt;Mongolia&lt;/st1:country-region&gt;&lt;/a&gt;&lt;/st1:place&gt; &lt;/p&gt;&lt;p&gt;&lt;b&gt;September 14&lt;sup&gt;th&lt;/sup&gt;:&lt;/b&gt; &lt;span&gt;&amp;nbsp;&lt;/span&gt;&lt;